			const confirmMsg = isChecked
				? 'Are you sure you want to turn ON maintenance mode?'
				: 'Are you sure you want to turn OFF maintenance mode?';

			if (!confirm(confirmMsg)) {
				// Revert checkbox state if canceled
				checkbox.prop('checked', !isChecked);
				return;
			}

			$('#maintenance-status').text('Processing...').css('color', 'orange');
	
