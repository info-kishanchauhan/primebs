   document.addEventListener('DOMContentLoaded', function() {
    const statusToggleSwitch = document.getElementById('statusToggleSwitch');
    const statusVisualIndicator = document.getElementById('statusVisualIndicator');
    const statusTextDisplay = document.getElementById('statusTextDisplay');
    
    // Get initial status from PHP
    const initialStatus = '<?php echo isset($_SESSION['LIVE_STATUS']) ? $_SESSION['LIVE_STATUS'] : "Off"; ?>';
    const isInitiallyOnline = (initialStatus.toLowerCase() === 'on');
    
    // Function to update UI based on status
    function updateStatusUI(isOnline) {
        if (isOnline) {
            statusVisualIndicator.className = 'status-visual-indicator status-visual-online';
            statusTextDisplay.textContent = 'Online';
            if (statusToggleSwitch) {
                statusToggleSwitch.checked = true;
            }
        } else {
            statusVisualIndicator.className = 'status-visual-indicator status-visual-offline';
            statusTextDisplay.textContent = 'Offline';
            if (statusToggleSwitch) {
                statusToggleSwitch.checked = false;
            }
        }
    }
    
    // Function to make AJAX call when status changes
    function sendStatusUpdate(isOnline) {
        // Show loading state
        const originalText = statusTextDisplay.textContent;
        const loadingText = isOnline ? 'Turning On...' : 'Turning Off...';
        statusTextDisplay.textContent = loadingText;
        
        if (statusToggleSwitch) {
            statusToggleSwitch.disabled = true;
        }
        
        var objFormData = {
            status: isOnline ? 'On' : 'Off',
            timestamp: new Date().getTime()
        };
        
        var objMyPost = AJAX_Post("<?php echo $this->url('login', array('action'=>'updateStatus'));?>", objFormData);
        
        // Handle response
        if (objMyPost.ERR_NO === 0) {
            if (objMyPost.DATA.DBStatus === 'OK') {
                // Update UI
                updateStatusUI(isOnline);
                
                // Show success message
                const statusText = isOnline ? 'Online' : 'Offline';
               
				mySmallAlert('Success', `Status successfully changed to ${statusText}`, 1);
                
                console.log(`Status updated to: ${isOnline ? 'Online' : 'Offline'}`);
            } else {
                // Revert UI if update failed
                updateStatusUI(!isOnline);
                
                // Show error message
                alert('Failed to update status. Please try again.');
                
                console.error('Status update failed:', objMyPost.DATA);
            }
        } else {
            // Revert UI on AJAX error
            updateStatusUI(!isOnline);
            
            // Show error message
            alert('Network error. Please check your connection and try again.');
            
            console.error('AJAX error:', objMyPost);
        }
        
        // Re-enable switch
        if (statusToggleSwitch) {
            statusToggleSwitch.disabled = false;
        }
    }
    
    // Event listener for switch change
    if (statusToggleSwitch) {
        statusToggleSwitch.addEventListener('change', function() {
            const isOnline = this.checked;
            const isChecked = this.checked;
            
            // Show confirmation dialog
            const confirmMsg = isChecked
                ? 'Are you sure you want to turn ON your status?'
                : 'Are you sure you want to turn OFF your status?';
            
            if (!confirm(confirmMsg)) {
                // Revert checkbox state if canceled
                this.checked = !isChecked;
                return;
            }
            
            // Make AJAX call to notify server of status change
            sendStatusUpdate(isOnline);
        });
    }
    
    // Add click handler for the status indicator
    if (statusVisualIndicator) {
        statusVisualIndicator.addEventListener('click', function() {
            if (statusToggleSwitch) {
                const currentStatus = statusToggleSwitch.checked;
                statusToggleSwitch.checked = !currentStatus;
                
                // Trigger change event
                const event = new Event('change');
                statusToggleSwitch.dispatchEvent(event);
            }
        });
        
        // Add cursor pointer for better UX
        statusVisualIndicator.style.cursor = 'pointer';
    }
    
    // Initialize the UI based on initial status
    updateStatusUI(isInitiallyOnline);
    
    // Optional: Add hover effect to status indicator
    if (statusVisualIndicator) {
        statusVisualIndicator.addEventListener('mouseenter', function() {
            this.style.transform = 'translateY(-2px)';
            this.style.transition = 'transform 0.2s';
        });
        
        statusVisualIndicator.addEventListener('mouseleave', function() {
            this.style.transform = 'translateY(0)';
        });
    }
});

