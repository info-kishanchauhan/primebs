		$('#fromDate').on('dp.change', function (e) {
			$('#toDate').val('');
			var fromDate = moment(e.date).startOf('month');  // Ensure `fromDate` is a Moment object and at the start of the month

			// Calculate the max date for toDate as 2 months ahead of the selected fromDate
			var maxToDate = moment(fromDate).add(2, 'months').endOf('month');  // `maxToDate` is a Moment object

			// Check if the calculated maxToDate exceeds the overall maxDate limit
			var maxAllowedDate = moment().subtract(2, 'months').endOf('month'); // This is the global max date limit
			if (maxToDate.isAfter(maxAllowedDate)) {
				maxToDate = maxAllowedDate;  // Restrict maxToDate to the overall max date
			}

			// Ensure the min date for 'toDate' is not earlier than 'fromDate'
			$('#toDate').data('DateTimePicker').minDate(fromDate.toDate());
			// Set the new max date for toDate picker
			$('#toDate').data('DateTimePicker').maxDate(maxToDate.toDate());

			
			$('#toDate').val(fromDate.format('MMMM YYYY'));
		});
  
		
