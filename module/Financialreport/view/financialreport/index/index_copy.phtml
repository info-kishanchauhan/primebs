		function enbleDisableGenerateTab() {
		  const isFirstTabActive = $('.process-model li:first').hasClass('active');
		  const isDedicated = $('#useDedicatedReport').hasClass('like-btn-primary');
		  const $generateTab = $('.generate_tab');
		  const $nextBtn = $('#nextStepButtonPlugin2');

		  if (isFirstTabActive) {
			$generateTab.css({ 'pointer-events': 'none', 'color': '#888' });
			$nextBtn.prop('disabled', true);
			return;
		  }

		  let enable = false;

		  if (isDedicated) {
			// Fast check for full catalog
			if ($('#dedicatedReportAllCatalog').is(':checked')) {
			  enable = true;
			} else {
			  // Optimized selector: select2 selects only with selected values
			  const hasSelected = $('#selectDedicatedMyReport select').filter(function () {
				return this.selectedOptions && this.selectedOptions.length > 0;
			  }).length > 0;

			  enable = hasSelected;
			}
		  } else {
			enable = true; // Multiple Reports mode
		  }

		  $generateTab.css({
			'pointer-events': enable ? '' : 'none',
			'color': enable ? '' : '#888'
		  });

		  $nextBtn.prop('disabled', !enable);
		}
